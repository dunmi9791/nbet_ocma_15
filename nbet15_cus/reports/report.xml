<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Account Move -->
    <record id="report_account_move_journal_voucher" model="ir.actions.report">
        <field name="name">Payment Voucher</field>
        <field name="model">account.payment</field>
        <field name="binding_type">report</field>
        <field name="binding_view_types">list,form</field>
        <field name="binding_model_id" ref="account.model_account_payment"/>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">nbet15_cus.report_journal_voucher</field>
        <field name="report_file">nbet15_cus.report_journal_voucher</field>
        <field name="print_report_name">'Payment Voucher - %s' % object.name</field>
    </record>
    <record id="report_account_move_voucher" model="ir.actions.report">
        <field name="name">Voucher</field>
        <field name="model">account.payment</field>
        <field name="binding_type">report</field>
        <field name="binding_view_types">list,form</field>
        <field name="binding_model_id" ref="account.model_account_payment"/>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">nbet15_cus.report_payment_voucher</field>
        <field name="report_file">nbet15_cus.report_payment_voucher</field>
        <field name="print_report_name">'Voucher - %s' % object.name</field>
    </record>
</odoo>